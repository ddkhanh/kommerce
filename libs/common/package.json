{
  "name": "@lib/common",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": false,
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {    
    "prebuild": "rimraf lib & yarn gen",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen": "proto-loader-gen-types -I=../protobuf --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/ user.proto",
    "start": "nest start",
    "start:dev": "nest build & nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.3.5",
    "@grpc/proto-loader": "0.6.4"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  }
}
